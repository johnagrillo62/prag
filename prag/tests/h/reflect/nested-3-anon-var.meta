}

// Field metadata
inline const auto fields = std::make_tuple(
    meta::Field< ::A, &::A::x>("x"),
    meta::Field< ::A, nullptr, Accessors<&get_b_y, &set_b_y>>("b.y", Accessors<&get_b_y, &set_b_y>{}
    );

